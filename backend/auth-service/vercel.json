{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/auth/login",
        "methods": ["POST", "OPTIONS"],
        "dest": "src/main.ts",
        "headers": {
          "Access-Control-Allow-Origin": "https://fjonic-admin.vercel.app",
          "Access-Control-Allow-Methods": "POST,OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      }
    ]
  }