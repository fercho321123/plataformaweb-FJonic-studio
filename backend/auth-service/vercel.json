{
    "version": 2,
    "builds": [{ "src": "src/main.ts", "use": "@vercel/node" }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "headers": {
          "Access-Control-Allow-Origin": "https://fjonic-admin.vercel.app",
          "Access-Control-Allow-Credentials": "true"
        }
      }
    ]
  }